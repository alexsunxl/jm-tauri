{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jm",
  "version": "0.1.24",
  "identifier": "com.aa.jm",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "jm",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": [
          "$EXE/jmcomic-cache/read/*",
          "$EXE/jmcomic-cache/read/**",
          "$EXE/../jmcomic-cache/read/*",
          "$EXE/../jmcomic-cache/read/**",
          "$APP/jmcomic-cache/read/*",
          "$APP/jmcomic-cache/read/**",
          "$APPCACHE/jmcomic-cache/read/*",
          "$APPCACHE/jmcomic-cache/read/**"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
